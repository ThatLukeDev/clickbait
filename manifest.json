{
    "manifest_version": 3,
    "name": "Clickbait",
    "description": "Add an image over all images on a webpage.",
    "version": "1.0.0",
    "action": {
        "default_popup": "index.html",
        "default_icon": "favicon.png"
    },
    "permissions": [
        "storage"
    ],
    "content_scripts": [
        {
            "js": ["content.js"],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}
